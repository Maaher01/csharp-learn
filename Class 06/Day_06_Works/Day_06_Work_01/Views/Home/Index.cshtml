@model IEnumerable< Day_06_Work_01.Controllers.Student >
@{
    ViewBag.Title = "Index";
}

<div class="my-2">
    @foreach(var s in Model)
            {
                <div>@s.Name</div>
                <div>@s.Id</div>
            }
</div>

<p class="my-2">This page is authored by @ViewBag.Author from @ViewData["City"].</p>

@for (var i=1; i<=5; i++)
{
    <span>@i</span><br />
}
